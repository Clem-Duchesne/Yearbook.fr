<div id='layout1' class="layout {% if page.layout != 'layout1' %}hide{%else%}activLayout{% endif %}">
    <div>
        <div id="h" class='zoneImage' data-image="image1">
        {% for content in contents %}
            {% if page.id == content.pages_id and content.cellule == "image1" and content.layout == 'layout1' %}
                <img src="{{assetA('img/photo/mini/') ~ content.image}}" class="imageOnPage" draggable="true">
            {% endif %}
        {% endfor %}
        </div>
        <div id="v" class='zoneImage' data-image="image2">
        {% for content in contents %}
            {% if page.id == content.pages_id and content.cellule == "image2" and content.layout == 'layout1' %}
                <img src="{{assetA('img/photo/mini/') ~ content.image}}" class="imageOnPage" draggable="true">
            {% endif %}
        {% endfor %}
        </div>
    </div>
    <div>
        <div id="v" class='zoneImage' data-image="image3">
        {% for content in contents %}
            {% if page.id == content.pages_id and content.cellule == "image3" and content.layout == 'layout1' %}
                <img src="{{assetA('img/photo/mini/') ~ content.image}}" class="imageOnPage" draggable="true">
            {% endif %}
        {% endfor %}
        </div>
        <div id="h" class='zoneImage' data-image="image4">
        {% for content in contents %}
            {% if page.id == content.pages_id and content.cellule == "image4" and content.layout == 'layout1' %}
                <img src="{{assetA('img/photo/mini/') ~ content.image}}" class="imageOnPage" draggable="true">
            {% endif %}
        {% endfor %}
        </div>
    </div>
</div>

<div id='layout2' class="layout {% if page.layout != 'layout2' %}hide{%else%}activLayout{% endif %}">
    <h1 data-style="title" name="title" {% for content in contents %}{% if page.id == content.pages_id and content.cellule == "title" and content.layout == 'layout2' %}class="text title {{content.font}}"{% endif %}{% endfor %}>{% for content in contents %}{% if page.id == content.pages_id and content.cellule == "title" and content.layout == 'layout2' %}{{content.text}} {% endif %}{% endfor %}</h1>
    <div class='zoneImage' data-image="image1">
        {% for content in contents %}
            {% if page.id == content.pages_id and content.cellule == "image1" and content.layout == 'layout2' %}
                <img src="{{assetA('img/photo/mini/') ~ content.image}}" class="imageOnPage" draggable="true">
            {% endif %}
        {% endfor %}
        </div>
    <p data-style="bottom-text" name="bottom-text" {% for content in contents %}{% if page.id == content.pages_id and content.cellule == "bottom-text" and content.layout == 'layout2' %}class="text bottom-text {{content.font}}"{% endif %}{% endfor %}>{% for content in contents %}{% if page.id == content.pages_id and content.cellule == "bottom-text" and content.layout == 'layout2' %}{{content.text}} {% endif %}{% endfor %}</p>
</div>

